<!doctype html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Invate</title>
    <% include dependence.ejs %>
    <link rel="stylesheet" href="/stylesheets/invate.min.css"/>
    <script src="/javascripts/invate.js"></script>
</head>
<body>
    <div class="logon">
        <h4>登陆以加入团队</h4>
        <span class="notice"><%= notice || "" %></span>
        <input type="text" class="account form-control" placeholder="用户名" <%= notice ? 'disabled' : '' %> />
        <input type="password" class="password form-control" placeholder="密码" <%= notice ? 'disabled' : '' %> />
        <span class="notice"></span>
        <button class="btn btn-primary" <%= notice ? 'disabled' : '' %> >确定</button>
    </div>
</body>
</html>